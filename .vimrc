set encoding=utf-8
set fileencodings=utf-8,cp950

syntax on
syntax enable
set noeb
set nu
set ai
set hlsearch
set ruler
set cursorline
set ic
set scrolloff=3

set smartindent
set expandtab
set tabstop=8
set shiftwidth=4
set softtabstop=0 smarttab

"set paste
set pastetoggle=<F12>

set autoread
set showmatch

filetype plugin indent on

colo torte
